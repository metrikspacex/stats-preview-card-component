{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2020"
    ],
    "module": "ES6",
    "moduleResolution": "nodenext",
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./public/scripts",
    "resolveJsonModule": true,
    "rootDir": "./src/",
    "skipLibCheck": true,
    "strict": true,
    "target": "ES6",
    "useDefineForClassFields": true
  }
}
